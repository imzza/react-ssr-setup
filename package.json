{
  "name": "webpack-test",
  "version": "1.0.0",
  "description": "React server side rendering setup",
  "author": "Maxime Joly",
  "license": "MIT",
  "scripts": {
    "analyze": "npm run build:with-stats && npm run start:analyzer",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.tooling.json ts-node scripts/build",
    "build:with-stats": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.tooling.json webpack --config config/webpack/config.client.ts --profile --json > build/client/static/bundle-stats.json",
    "build:storybook": "build-storybook",
    "start": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.tooling.json ts-node scripts/start",
    "start:analyzer": "webpack-bundle-analyzer build/client/static/bundle-stats.json",
    "start:storybook": "start-storybook -p 6006",
    "clean": "rimraf build",
    "depgraph": "depcruise -c .dependency-cruiser.js --exclude \"node_modules/*\" --output-type dot src | dot -T svg > dependency-graph.svg",
    "lint": "concurrently \"npm:lint:js\" \"npm:lint:css\" \"npm:lint:sass\"",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:css": "stylelint src/**/*.css --syntax css --allow-empty-input ",
    "lint:sass": "stylelint src/**/*.{scss,sass} --syntax scss --allow-empty-input"
  },
  "dependencies": {
