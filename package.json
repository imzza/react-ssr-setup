{
  "name": "webpack-test",
  "version": "1.0.0",
  "description": "React server side rendering setup",
  "author": "Maxime Joly",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.tooling.json ts-node scripts/start",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.tooling.json ts-node scripts/build",
    "build:stats": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.tooling.json webpack --config config/webpack/config.client.ts --profile --json > build/client/static/bundle-stats.json",
    "build:storybook": "build-storybook",
    "clean": "rimraf build",
    "analyze": "yarn-or-npm run build:stats && webpack-bundle-analyzer build/client/static/bundle-stats.json",
    "storybook": "start-storybook -p 6006",
    "depgraph": "depcruise -c .dependency-cruiser.js --exclude \"node_modules/*\" --output-type dot src | dot -T svg > dependency-graph.svg",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.tooling.json ts-node scripts/test",
    "test:coverage": "yarn-or-npm run test --coverage",
    "lint": "concurrently \"yarn-or-npm run lint:js\" \"yarn-or-npm run lint:css\" \"yarn-or-npm run lint:sass\"",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:css": "stylelint src/**/*.css --syntax css --allow-empty-input ",
    "lint:sass": "stylelint src/**/*.{scss,sass} --syntax scss --allow-empty-input",
    "format": "yarn-or-npm run format:prettier && yarn-or-npm run format:stylelint && yarn-or-npm run format:eslint",
    "format:prettier": "prettier --write .",
    "format:eslint": "eslint --fix .",
    "format:stylelint": "stylelint src/**/*.{css,scss,sass}",
    "plop": "cross-env TS_NODE_PROJECT=tsconfig.tooling.json plop --plopfile config/plop/index.ts"
  },
